{
  "$help": "https://aka.ms/terminal-documentation",
  "$schema": "https://aka.ms/terminal-profiles-schema",
  "actions": [
    {
      "command": {
        "action": "copy",
        "singleLine": false
      },
      "keys": "ctrl+c"
    },
    {
      "command": "unbound",
      "keys": "ctrl+f12"
    },
    {
      "command": "unbound",
      "keys": "ctrl+shift+f"
    },
    {
      "command": "unbound",
      "keys": "ctrl+alt+8"
    },
    {
      "command": "unbound",
      "keys": "ctrl+alt+7"
    },
    {
      "command": "unbound",
      "keys": "ctrl+shift+9"
    },
    {
      "command": "paste",
      "keys": "ctrl+v"
    },
    {
      "command": "find"
    },
    {
      "command": {
        "action": "splitPane",
        "split": "auto",
        "splitMode": "duplicate"
      },
      "keys": "alt+shift+d"
    },
    {
      "command": "toggleFocusMode",
      "keys": "f12"
    }
  ],
  "alwaysOnTop": false,
  "alwaysShowTabs": true,
  "centerOnLaunch": true,
  "copyFormatting": "none",
  "copyOnSelect": false,
  "defaultProfile": "{PROFILE_KEY}",
  "firstWindowPreference": "defaultProfile",
  "initialCols": 190,
  "initialPosition": ",",
  "initialRows": 60,
  "launchMode": "focus",
  "newTabMenu": [
    {
      "type": "remainingProfiles"
    }
  ],
  "profiles": {
    "defaults": {
      "backgroundImage": "C:/path/to/your/image",
      "opacity": 30,
      "padding": "10",
      "startingDirectory": "F:\\your_custom_path",
      "useAcrylic": true
    },
    "list": [
      {
        "altGrAliasing": true,
        "antialiasingMode": "grayscale",
        "backgroundImage": null,
        "backgroundImageOpacity": 0.34,
        "closeOnExit": "automatic",
        "colorScheme": "Campbell",
        "commandline": "C:your_custom_path",
        "cursorHeight": 20,
        "cursorShape": "bar",
        "experimental.retroTerminalEffect": false,
        "font": {
          "face": "FiraCode Nerd Font",
          "size": 12.0,
          "weight": "semi-light"
        },
        "guid": "PROFILE_KEY",
        "hidden": false,
